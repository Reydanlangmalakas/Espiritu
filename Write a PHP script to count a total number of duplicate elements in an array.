<?php
for ($i=0; $i <= 2; $i++) {
        $line = readline("Enter Number: ");
        readline_add_history($line);
}
$input= array(5,1,1);

$count_values = array();
foreach ($input as $a) {

     @$count_values[$a]++;

}
echo 'Total number of duplicate elements found in the array is : '.count($count_values);
?>
